{
    "headers": [
      {
        "source": "/fonts/(.*)/(.*)\\.(ttf|otf|woff|woff2)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          },
          {
            "key": "Content-Type",
            "value": "font/ttf"
          }
        ]
      }
    ]
  }